[credential "https://dev.azure.com"]
	useHttpPath = true
[user]
	name = Josh Johanning
	email = joshjohanning@github.com
	signingkey = /Users/joshjohanning/.ssh/id_rsa.pub
[github]
	user = joshjohanning
[pager]
	branch = false
[pull]
	ff = true
[init]
	defaultBranch = main
[credential]
	helper = 
	helper = /usr/local/share/gcm-core/git-credential-manager
[push]
	autoSetupRemote = true
[core]
	editor = code --wait
[commit]
	gpgsign = true
[gpg]
